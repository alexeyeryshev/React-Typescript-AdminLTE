{
  "extends": ["tslint:latest"],
  "rules": {
    "ter-indent": [true, 2],
    "indent": [true, "spaces"],
    "no-unused-variable": false,
    "ordered-imports": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "variable-name": false
  },
  "rulesDirectory": ["./scripts/tslint/"]
}